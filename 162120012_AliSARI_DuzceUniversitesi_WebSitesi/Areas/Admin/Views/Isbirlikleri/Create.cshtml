@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model _162120012_AliSARI_DuzceUniversitesi_WebSitesi.Models.Isbirlikleri

@{
    ViewData["Title"] = "Protokol İmzalanan Firma Ekle";
}
<br />
<h2>Protokol İmzalanan Firma Ekle</h2>
<hr />

<form asp-action="Create" asp-controller="Isbirlikleri" method="post" class="form-horizontal">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="form-group">
        <label class="control-label col-md-3">Protokol İmzalanan Firma Adları</label>
        <div class="col-md-9">
            <input type="text" asp-for="ProtokolImzalananFirmalar" class="form-control text-info" />
            <span asp-validation-for="ProtokolImzalananFirmalar" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-3 col-md-9">
            <button class="btn btn-default bi bi-folder-plus guncellemebuton" style="margin-left: 5px; border: 2px solid #4db3ff;">
                &nbsp; Protokol İmzalanan Firma Ekle
            </button>
        </div>
    </div>
</form>
<a asp-action="Index" asp-controller="Isbirlikleri" class="btn btn-default fas fa-backward" style="color: green"> Geri Dön</a>
